#include <stdio.h>
int
main ()
{
  //Pattern is printed
  printf ("*\n**\n***\n****\n*****\n");
  return 0;
}

Square
#include <stdio.h>
int 
main() 
{
  //Square is printed
  printf("IIIIIIII\nIIIIIIII\nIIIIIIII\nIIIIIIII\n");
  return 0;
}

Diamond
#include <stdio.h>
int 
main() 
{
  //Diamond is printed
  printf("    I    \n   III  \n  IIIII    \n IIIIIII\n  IIIII\n   III\n    I\n");
  return 0;
}
